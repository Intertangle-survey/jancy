project       = Jancy
description   = Jancy Standard Library Reference Documentation
version       = ${JANCY_VERSION_MAJOR}.${JANCY_VERSION_MINOR}.${JANCY_VERSION_REVISION}
outputdir     = ${JANCY_DOC_DIR}/jnc_std/html
outputformats = HTML

headerdirs  = \
	${JANCY_ROOT_DIR}/src/jnc_io_base/jnc \
	${JANCY_ROOT_DIR}/src/jnc_io_ssh/jnc \
	${JANCY_ROOT_DIR}/src/jnc_io_pacp/jnc
	
sourcedirs  = ${CMAKE_CURRENT_LIST_DIR}/src/jnc_std 	

headers.fileextensions = *.jnc
sources.fileextensions = "*.qdoc *.jnc"

HTML.stylesheets  = style/offline.css
HTML.headerstyles = "<link rel=\"stylesheet\" type=\"text/css\" href=\"style/offline.css\"/>\n"
HTML.extraimages  = \
	images/ico_out.png \
	images/ico_note.png \
	images/ico_note_attention.png \
	images/btn_prev.png \
	images/btn_next.png \
	images/bullet_dn.png \
	images/bullet_sq.png \
	images/bgrContent.png
