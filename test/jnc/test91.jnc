int main ()
{
	printf ("main ()\n");

	char const* p = "bazzzbzz";

	jnc.RegExState state;
	regex switch (state, p)
	{
	case "gr+":
		printf ("gr+ matched (match: %d, %d; cur: %d)\n", state.m_match.m_offset, state.m_match.m_length, state.m_currentOffset);
		break;

	case ".*bz+":
		printf ("bz+ matched (match: %d, %d; cur: %d)\n", state.m_match.m_offset, state.m_match.m_length, state.m_currentOffset);
		break;

	default:
		printf ("nothing matched (cur: %d)\n", state.m_currentOffset);
		break;
	}

	return 0;
}

