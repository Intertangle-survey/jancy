dynamic struct S
{
	char m_a;
	char m_string1 [m_a];
	char m_b;
	char m_string2 [strlen (m_string2) + 1];
	char m_c;
}

int main ()
{
	char buffer [] = { 4, 'a', 'b', 'c', 0, 10, 'd', 'e', 'f', 'g', 0, 20 };
	S* s = (S*) buffer;	

	printf (
		"s.m_a = %d\n"
		"s.m_b = %d\n"
		"s.m_c = %d\n", 
		s.m_a,
		s.m_b,
		s.m_c
		);

	printf ("s     = %p\n", s);

	s++;

	printf ("s + 1 = %p\n", s);

	return 0;
}
