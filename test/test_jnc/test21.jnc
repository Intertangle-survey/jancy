class MyScheduler: jnc.Scheduler
{
	override schedule (function* f ())
	{
		printf ("MyScheduler.schedule ()\n");
		f ();
	}
}

foo (int x)
{
	printf ("foo (%d)\n", x);
}

int main ()
{
	static MyScheduler scheduler;

	printf ("main ()\n");
	(foo @ scheduler) (10);

	return 0;
}

