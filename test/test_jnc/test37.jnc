struct Point
{
	int m_x;
	int m_y;
}

foo (Point* point)
{
	printf ("foo ({%d, %d})\n", point.m_x, point.m_y);
}

//.............................................................................

int main ()
{
	printf ("main\n");

	Point point = { 10, 20 };
	foo (point);

	return 0; 
}

