struct Point
{
	int m_x;
	int m_y;
	int m_z;
	int m_w;
}

opaque class OpaqueTest
{
	OpaqueTest* operator new (
		int x, 
		int y
		);

	thiscall foo ();
	Point thiscall bar (int x);
}

int main ()
{
	printf ("main ()\n");

	OpaqueTest* a = new OpaqueTest (1, 2);
	a.foo ();
	Point point = a.bar (10);

	printf (
		"point = { %d, %d, %d, %d }\n", 
		point.m_x, 
		point.m_y, 
		point.m_z, 
		point.m_w
		);

	return 10; 
}

