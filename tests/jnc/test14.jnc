event g_onChanged ();

property g_prop
{
	bindable event m_onChanged () => g_onChanged;
	int autoget m_value;

	set (int x)
	{
		m_value = x;
		m_onChanged ();
	}
}

int main ()
{
	bindingof (g_prop) += foo;	

	g_onChanged ();

	return 0;
}
 
